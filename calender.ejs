  <style>
    table {
      border: 2px solid grey;
      margin: auto;
      border-radius: 10px;
    }

    th,
    td {
      border-collapse: collapse;
      padding: 10px 20px;
      text-align: center;
      border: 2px solid black;
    }

    td {
      height: 130px;
      width: 130px;
      border: 2px solid grey;
    }

    th {
      background-color: #0A1828;
      color: white;
    }


    td:hover {
      background-color: rgb(199, 227, 199);
    }
  </style>
